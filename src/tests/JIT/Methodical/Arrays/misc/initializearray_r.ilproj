<Project Sdk="Microsoft.NET.Sdk.IL">
  <PropertyGroup>
    <Optimize>true</Optimize>

    <!-- NativeAOT doesn't support non-intrinsic expansion of InitializeArray -->
    <NativeAotIncompatible>true</NativeAotIncompatible>
  </PropertyGroup>
  <PropertyGroup>
    <DebugType>PdbOnly</DebugType>
    <Optimize>True</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="initializearray_r.il" />
    <Compile Include="initializearray.il" />
  </ItemGroup>
</Project>
